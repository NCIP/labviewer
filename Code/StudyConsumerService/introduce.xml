      <ns4:Exception description="" name="StudyCreationException" qname="ns8:StudyCreationException" xmlns:ns8="http://ccts.nci.nih.gov/StudyConsumer/types"/>
     </ns4:Exceptions>
    </ns4:Method>
    <ns4:Method description="Commit a create study operation" isImported="false" isProvided="false" name="commit" xsi:type="ns4:MethodType">
     <ns4:Inputs>
