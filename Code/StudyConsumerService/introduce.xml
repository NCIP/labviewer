    </ns4:Method>
    <ns4:Method description="Create a new Protocol" isImported="false" isProvided="false" name="createStudy" xsi:type="ns4:MethodType">
     <ns4:Inputs>
      <ns4:Input qName="ns6:study" description="" isArray="false" name="study" xmlns:ns6="gme://ccts.cabig/1.0/gov.nih.nci.cabig.ccts.domain"/>
     </ns4:Inputs>
     <ns4:Output qName="void" description="" isArray="false"/>
