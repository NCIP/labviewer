<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD tiles Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/tiles-config_1_1.dtd">


<tiles-definitions>

	<!-- Base layouts -->
	<definition name="Base" page="/pages/layout/BaseLayout.jsp">
		<put name="OrgHeader" value="/pages/header/OrgHeader.jsp" />
		<put name="AppHeader" value="/pages/header/AppHeader.jsp" />
		<put name="AppMenu" value="/pages/menu/MainMenu.jsp"/>
		<put name="AppAdminMenu" value="/pages/menu/SuperAdminMenu.jsp"/>		
		<put name="AppFooter" value="/pages/footer/AppFooter.jsp" />
		<put name="OrgFooter" value="/pages/footer/OrgFooter.jsp" />
		<put name="Title" type="string"	value="CTOM Lab Viewer" />
	</definition>

	<!-- Pages Begin -->
	
	<!-- Login Pages -->
	<definition name="Tiles.Login.Page" extends="Base">
		<put name="Content" value="/pages/content/main/Login.jsp" />
	</definition>
	
	<!-- Footer Pages -->
	<definition name="Tiles.Footer.Accessibility" extends="Base">
		<put name="Content" value="/pages/content/footer/Accessibility.jsp" />
	</definition>
	<definition name="Tiles.Footer.ContactUs" extends="Base">
		<put name="Content" value="/pages/content/footer/ContactUs.jsp" />
	</definition>
	<definition name="Tiles.Footer.Privacy" extends="Base">
		<put name="Content" value="/pages/content/footer/Privacy.jsp" />
	</definition>
	<definition name="Tiles.Footer.ApplicationSupport" extends="Base">
		<put name="Content" value="/pages/content/footer/ApplicationSupport.jsp" />
	</definition>
	<definition name="Tiles.Footer.Disclaimer" extends="Base">
		<put name="Content" value="/pages/content/footer/Disclaimer.jsp" />
	</definition>
	
	<!-- Error Pages -->
	<definition name="Tiles.500.Error.Page" extends="Base">
		<put name="Content" value="/pages/content/main/Error500.jsp" />
	</definition>
	<definition name="Tiles.500.Error.DetailsPage" extends="Base">
		<put name="Content" value="/pages/content/main/ErrorDetails.jsp" />
	</definition>
	
	<!-- Home Pages -->
	<definition name="Tiles.Home.Page" extends="Base">
		<put name="Content" value="/pages/content/home/MainHome.jsp" />
	</definition>
	<definition name="Tiles.SuperAdmin.Home.Page" extends="Base">
		<put name="Content" value="/pages/content/home/SuperAdminHome.jsp" />
	</definition>
	<definition name="Tiles.LabActivities.Home" extends="Base">
		<put name="Content" value="/pages/content/home/LabActivitiesHome.jsp" />
	</definition>

	<!-- Detail Screens -->
	<definition name="Tiles.LabActivities.Detail.Screen" extends="Base">
		<put name="Content" value="/pages/content/detailScreen/LabActivitiesDetailScreen.jsp" />
	</definition>	
	
	<!-- Search Result Pages -->
	<definition name="Tiles.LabActivities.Search.Result" extends="Base">
		<put name="Content" value="/pages/content/searchResult/LabActivitiesSearchResult.jsp" />
	</definition>

</tiles-definitions>