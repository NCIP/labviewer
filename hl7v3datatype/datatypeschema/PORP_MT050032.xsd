                     </mif:type>
                  </mif:attribute>
               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="assignedPerson" type="PORP_MT050032.Person" nillable="true" minOccurs="0" maxOccurs="1">
            <xs:annotation>
               <xs:appinfo>
                  <mif:targetConnection name="assignedPerson" minimumMultiplicity="0" maximumMultiplicity="1" isMandatory="false" sortKey="1">
