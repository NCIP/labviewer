                     <mif:derivationSupplier staticModelDerivationId="2" className="AssignedEntity" attributeName="telecom"/>
                     <mif:derivationSupplier staticModelDerivationId="3" className="AssignedEntity" attributeName="telecom"/>
                     <mif:type name="SET">
