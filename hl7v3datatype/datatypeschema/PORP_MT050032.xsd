               </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="assignedEntity" type="PORP_MT050032.AssignedEntity" nillable="true" minOccurs="1" maxOccurs="1">
            <xs:annotation>
               <xs:appinfo>
                  <mif:targetConnection name="assignedEntity" minimumMultiplicity="1" maximumMultiplicity="1" isMandatory="false" sortKey="1">
                     <mif:annotations/>
                     <mif:derivationSupplier staticModelDerivationId="1" className="Participation" associationEndName="role"/>
                     <mif:derivationSupplier staticModelDerivationId="2" className="Participation" associationEndName="assignedEntity"/>
                     <mif:derivationSupplier staticModelDerivationId="3" className="Participation" associationEndName="assignedEntity"/>
                     <mif:participantClass>
