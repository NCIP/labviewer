<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caxchange.ctom.viewer.beans">
	<class name="LabViewerStatus" table="Lab_Viewer_Status" lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		  <id name="id" type="integer" column="ID">
            <generator class="sequence">
                <param name="sequence">LAB_VIEWER_STATUS_SEQ</param>
            </generator>
        </id>
		<!-- Attributes mapping for the LaboratoryResult class -->
		<property name="adverseEventIndicator" type="string" column="ADVERSE_EVENT_INDICATOR"/>
		<property name="adverseEventSentDate" type="timestamp" column="ADVERSE_EVENT_SENT_DATE"/>
		<property name="cdmsIndicator" type="string" column="CDMS_INDICATOR"/>
		<property name="cdmsSentDate"  type="timestamp" column="CDMS_SENT_DATE"/>
		<property name="clinicalResultId" type="integer" column="CLINICAL_RESULT_ID"/>
		<property name="ctomInsertDate"  type="timestamp" column="CTOM_INSERT_DATE"/>
		<property name="ctomUpdateDate"  type="timestamp" column="CTOM_UPDATE_DATE"/>
	</class>
</hibernate-mapping>