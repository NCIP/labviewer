<!--
	
	This is the definition of a virtual module called bda-download that has a dependency of bda-utils.  
	This virtual project causes the download of bda-utils from the NCICB Ivy Repository.  
	This file currently points to the 1.0 version of bda-utils if you want to use another version change the rev="1.0" 
	to the desired version.
-->
<ivy-module version="1.0">
	<info organisation="cbiit" module="ctomlabviewer" />
	<configurations>
		<conf name="compile" />
		<conf name="default"/>
		<conf name="client" />
		<conf name="service"/>
		<conf name="runtime" />
		<conf name="container" />
		<conf name="test" extends="runtime" />
		<conf name="test" extends="compile" />
		<conf name="test-pmd" />
		<conf name="test-junit" />
		<conf name="test-cobertura" />
		<conf name="test-checkstyle" />
		<conf name="test-simian" />
		<conf name="test-selenium" />
	</configurations>

	<publications></publications>

	<dependencies>
		<!-- compile -->
		<dependency org="ehcache" 			name="ehcache" 					rev="1.2.4" 		conf="compile->master" />
		<dependency org="javax.servlet" 	name="servlet" 					rev="2.3.1"			conf="compile->master" />	
		<dependency org="org.hibernate" 	name="hibernate" 				rev="3.1.3" 		conf="compile->master" />
		<dependency org="postgresql" 		name="postgresql-jdbc3" 		rev="8.3-603" 		conf="compile->master" />
		<dependency org="oracle" 			name="ojdbc14" 					rev="10.2.0.4.0" 	conf="compile->master" />
		<dependency org="displaytag" 		name="displaytag" 				rev="1.2" 			conf="compile->master" />
		<!--  caGrid 1.3 jars -->
		<dependency org="caGrid" 			name="cql" 						rev="1.3" 			conf="compile->jars" />
		<dependency org="caGrid" 			name="core" 					rev="1.3" 			conf="compile->default"/>
		<dependency org="caGrid" 			name="introduce" 				rev="1.3" 			conf="compile->default" transitive="false" />
		<dependency org="caGrid" 			name="service-security-provider" rev="1.3" 			conf="compile->client" />
        <dependency org="caGrid" 			name="websso-client" 			rev="1.3" 			conf="compile->default" transitive="false" />
        <dependency org="caGrid" 			name="websso-client-common" 	rev="1.3" 			conf="compile->default" transitive="false"/>
        <dependency org="inspektr" 			name="inspektr" 				rev="0.7.0" 		conf="compile->default"/>
        <dependency org="caGrid" 			name="cds" 						rev="1.3" 			conf="compile->service"/>
		<!--  other caGrid dependent jars -->
		<dependency org="acegisecurity" 	name="acegi-security" 			rev="1.0.5" 		conf="compile->default"/>
		<dependency org="jasig" 			name="cas-client-core" 			rev="3.1.3" 		transitive="false"/>
		<dependency org="globus" 			name="wss4j" 					rev="noversion"		conf="compile->master"/>
        <dependency org="globus" 			name="jcogfx" 					rev="noversion" 	conf="compile->master"/>
		<dependency org="globus" 			name="jcog" 					rev="1.2" />
        <dependency org="springsource" 		name="spring" 					rev="2.5.6" 		conf="compile->master" />
		<dependency org="apache" 			name="struts" 					rev="1.1" 			conf="compile->master" />	
		<dependency org="apache" 			name="jarkarta-commons-httpclient-contrib" 	rev="3.0"/>
		<dependency org="apache" 			name="commons-logging" 			rev="1.1"/>
		<dependency org="apache" 			name="commons-beanutils" 		rev="1.7.0"/>
		<dependency org="apache" 			name="log4j" 					rev="1.2.15" 		conf="default" />
		<dependency org="apache" 			name="tomcat-servlet" 			rev="2.2"/>
		<dependency org="apache" 			name="axis" 					rev="1.4.2_10"/>
		<dependency org="apache" 			name="addressing" 				rev="1.0"/>
	    <dependency org="apache" 			name="log4j" 					rev="1.2.13" 		conf="compile->master" />
		<!--  ncicb jars -->
		<dependency org="ncicb" 			name="C3DGridService" 			rev="noversion" 	conf="compile->master" />
		<dependency org="ncicb" 			name="CoreServices" 			rev="3.1" 			conf="compile->master" />
		<dependency org="ncicb" 			name="CaXchangeRequestProcessor" rev="noversion" 	conf="compile->master" />
		<dependency org="ncicb" 			name="PAServices" 				rev="3.1" 			conf="compile->master" />
		<dependency org="ncicb" 			name="coppa-commons" 			rev="1.1-SNAPSHOT" 	conf="compile->master" />
		<dependency org="ncicb" 			name="sdk-client-framework" 	rev="noversion" />
        <dependency org="ncicb" 			name="sdk-core" 				rev="noversion"/>
        <dependency org="ncicb" 			name="wsrf-core" 				rev="noversion"/>
        
        <dependency org="gov.nih.nci.cabig.ctms" name="ctms-commons-suite-authorization" rev="0.4.2.RELEASE" conf="compile->runtime" >
            <exclude module="commons-lang" /> <!-- this excludes v1.0.1, display tag needs v2.x -->
            <exclude module="jdom" /> <!-- transitive dependency from csm to jdom doesn't work, so have to exclude and include below -->
            <exclude module="com.springsource.org.apache.commons.attributes" /> <!-- exclude because of invalid manifest -->
            <!-- exclude the remaining to avoid adding springsource repositories to ivysettings.xml -->
            <exclude module="com.springsource.org.apache.commons.lang" />
            <exclude module="com.springsource.org.hibernate" />
            <exclude module="com.springsource.org.hibernate.annotations" />
            <exclude module="com.springsource.javax.persistence" />
            <exclude module="org.springframework.beans" />
            <exclude module="org.springframework.context" />
            <exclude module="org.springframework.core" />
            <exclude module="org.springframework.orm" />
            <exclude module="org.springframework.transaction" />
            <exclude module="com.springsource.org.apache.tools.ant" />
            <exclude module="com.springsource.org.junit" />
            <exclude module="com.springsource.org.hsqldb" />
            <exclude module="slf4j-api"/>
        </dependency>
        <dependency org="org.slf4j" 		name="slf4j-log4j12" 			rev="1.5.5" 		conf="compile->master"/>
        <dependency org="org.slf4j" 		name="slf4j-api" 				rev="1.5.11" 		conf="compile->master"/>
        <dependency org="commons-lang" 		name="commons-lang" 			rev="2.4" 			conf="compile->master"/>
        <dependency org="c3p0" 				name="c3p0" 					rev="0.9.0" />
        <dependency org="dom4j" 			name="dom4j" 					rev="1.6.1" 		conf="compile->master">
           <exclude module="pull-parser"/>
        </dependency>

        <dependency org="stax" 				name="stax-api" 				rev="1.0" />
        <dependency org="jaxb" 				name="jaxb-impl" 				rev="2.0.1"/>
        <dependency org="javax.transaction" name="jta" 						rev="1.0.1B"/>
        <dependency org="org.antlr" 		name="antlr" 					rev="2.7.6"/>
        <dependency org="javax.activation" 	name="activation" 				rev="1.0.2"/>
        <dependency org="jakarta" 			name="commons-collections" 		rev="3.2"/>
        <dependency org="jakarta" 			name="commons-httpclient" 		rev="3.0.1"/>
	
		<!-- container -->
		<dependency org="postgresql" 		name="postgresql-jdbc3" 		rev="8.3-603" 		conf="container->master" />
		<dependency org="oracle" 			name="ojdbc14" 					rev="10.2.0.4.0" 	conf="container->master" />
		<dependency org="ehcache" 			name="ehcache" 					rev="1.2.4" 		conf="container->master" />
	    <dependency org="javax.servlet" 	name="servlet" 					rev="2.3.1"			conf="container->master" />

		<!-- test -->
		<dependency org="postgresql" 		name="postgresql-jdbc3" 		rev="8.3-603" 		conf="test->master" />
		<dependency org="oracle" 			name="ojdbc14" 					rev="10.2.0.4.0" 	conf="test->master" />
		<dependency org="org.antlr" 		name="antlr" 					rev="2.7.6" 		conf="test->master" />
		<dependency org="apache" 			name="log4j" 					rev="1.2.13" 		conf="test->master" />
		<dependency org="junit" 			name="junit" 					rev="4.1" 			conf="test->master" />
		
		<!-- other test targets -->
		<dependency org="checkstyle" 		name="checkstyle-all" 			rev="4.4" 			conf="test-checkstyle->compile" />
		<dependency org="cobertura" 		name="cobertura" 				rev="1.9" 			conf="test-cobertura->compile" />
		<dependency org="junit" 			name="junit" 					rev="4.1" 			conf="test-junit->compile" />
		<dependency org="openqa" 			name="selenium-server" 			rev="1.0-beta-1" 	conf="test-selenium->compile" />
		<dependency org="pmd" 				name="pmd" 						rev="4.2.2" 		conf="test-pmd->compile,runtime" />
		<dependency org="redhillconsulting" name="simian" 					rev="2.2.24" 		conf="test-simian->compile" />
	</dependencies>
</ivy-module>
