<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caxchange.ctom.viewer.beans">
	<class name="HealthcareSitePart" table="healthcare_site_prtcpnt"
		lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="integer" column="ID">
			<generator class="sequence">
				<param name="sequence">
					healthcare_site_prtcpnt_seq
				</param>
			</generator>
		</id>
		<!-- Attributes mapping for the LaboratoryResult class -->
		<property name="participantIdentifier" type="string"
			column="participant_identifier" />
		<property name="healthcareSiteId" type="integer"
			column="healthcare_site_id" />
		<property name="participantId" type="integer"
			column="participant_id" />
		<property name="securityKey" type="integer"
			column="security_key" />
		<property name="source" type="integer" column="SOURCE" />
		<property name="sourceExtractDate" type="timestamp"
			column="SOURCE_EXTRACT_DATE" />
		<property name="ctomInsertDate" type="timestamp"
			column="CTOM_INSERT_DATE" />
		<property name="ctomUpdateDate" type="timestamp"
			column="CTOM_UPDATE_DATE" />

		<many-to-one name="healthCareSite"
			class="gov.nih.nci.labhub.domain.HealthCareSite"
			column="healthcare_site_id" outer-join="false" not-null="true" insert="false" update="false" />
		<many-to-one name="participant"
			class="gov.nih.nci.labhub.domain.Participant"
			column="participant_id" outer-join="false" not-null="true"  insert="false" update="false"/>

	</class>
</hibernate-mapping>
