<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="gov.nih.nci.caxchange.ctom.viewer.beans">
	<class name="Users" table="csm_user"
		lazy="true" polymorphism="explicit">
		<cache usage="read-write" />
		<id name="id" type="integer" column="user_id">
			<generator class="sequence">
				<param name="sequence">
					CSM_USER_USER_ID_SEQ
				</param>
			</generator>
		</id>
		<!-- Attributes mapping for the LaboratoryResult class -->
		<property name="loginName" type="string"
			column="login_name" />
		<property name="firstName" type="string"
			column="first_name" />
		<property name="lastName" type="string"
			column="last_name" />
		<property name="organization" type="string"
			column="organization" />
		<property name="department" type="string" column="department" />
		<property name="title" type="string" column="title" />
		<property name="phoneNumber" type="string" column="phone_number" />
		<property name="password" type="string" column="password" />
		<property name="emailId" type="string" column="email_id" />
		<property name="startDate" type="timestamp" column="start_date" />
		<property name="endDate" type="timestamp" column="end_date" />
		<property name="updateDate" type="timestamp" column="update_date" />
		
	</class>
</hibernate-mapping>
