<!--
	
	This is the definition of a virtual module called bda-download that has a dependency of bda-utils.  
	This virtual project causes the download of bda-utils from the NCICB Ivy Repository.  
	This file currently points to the 1.0 version of bda-utils if you want to use another version change the rev="1.0" 
	to the desired version.
-->
<ivy-module version="1.0">
	<info organisation="cbiit" module="ctomlabviewer" />
	<configurations>
		<conf name="compile" />
		<conf name="default" />
		<conf name="client" />
		<conf name="service" />
		<conf name="runtime" />
		<conf name="container" />
		<conf name="test" extends="runtime" />
		<conf name="test" extends="compile" />
		<conf name="test-pmd" />
		<conf name="test-junit" />
		<conf name="test-cobertura" />
		<conf name="test-checkstyle" />
		<conf name="test-simian" />
		<conf name="test-selenium" />
	</configurations>

	<publications></publications>

	<dependencies>
		<!-- compile -->
		<dependency org="ehcache" name="ehcache" rev="1.2.4" conf="compile->master" />
		<dependency org="apache" name="struts" rev="1.1" conf="compile->master" />	
		<dependency org="javax.servlet" name="servlet" rev="2.3.1"	conf="compile->master" />	
		<dependency org="org.hibernate" name="hibernate" rev="3.1.3" conf="compile->master" />
		<dependency org="postgresql" name="postgresql-jdbc3" rev="8.3-603" conf="compile->master" />
		<dependency org="springsource" name="spring" rev="2.0.5" conf="compile->master" />
		<dependency org="apache" name="log4j" rev="1.2.13" conf="compile->master" />
		<dependency org="junit" name="junit" rev="4.1" conf="compile->master" />
		<dependency org="displaytag" name="displaytag" rev="1.2" conf="compile->master" />
		<dependency org="ncicb" name="CaXchangeRequestProcessor" rev="noversion" conf="compile->master" />
		<dependency org="ncicb" name="C3DGridService" rev="noversion" conf="compile->master" />
		<dependency org="ncicb" name="CoreServices" rev="noversion" conf="compile->master" />
		<dependency org="ncicb" name="smoketestservice" rev="0.1" conf="compile->master" />
		<dependency org="caGrid" name="cql" rev="1.2" />
		<dependency org="caGrid" name="core" rev="1.2" />
		<dependency org="cacore" name="sdk" rev="3.2.1"/>
		<dependency org="caGrid" name="caGrid-websso-client-1.2" rev="1.2" conf="default" />	
		<dependency org="caGrid" name="authentication-service" rev="1.2" conf="service" />
		<dependency org="caGrid" name="advertisement" rev="1.2" conf="default"/>
		<dependency org="caGrid" name="service-security-provider" rev="1.2" conf="service"/>
		<dependency org="caGrid" name="gridca" rev="1.2" conf="default"/>
		<dependency org="apache" name="tomcat-servlet" rev="2.2"/>
		<dependency org="apache" name="axis" rev="1.4.2_10"/>
		<dependency org="apache" name="axisant" rev="noversion"/>
		<dependency org="apache" name="axisurl" rev="noversion"/>
		<dependency org="globus" name="jcog" rev="1.2" />
		<dependency org="ncicb" name="sdk-client-framework" rev="noversion" />
        <dependency org="ncicb" name="sdk-core" rev="noversion"/>
        <dependency org="cagrid" name="servicesecurityprovider" rev="1.2"/>
        <dependency org="ncicb" name="wsrf-core" rev="noversion"/>
        <dependency org="apache" name="struts" rev="1.1"/>
        <dependency org="cglib" name="cglib" rev="2.1_3"/>
        <dependency org="ncicb" name="clm" rev="4.0"/>
        <dependency org="javax.transaction" name="jta" rev="1.0.1B"/>
        <dependency org="org.antlr" name="antlr" rev="2.7.6"/>
        <dependency org="javax.activation" name="activation" rev="1.0.2"/>
        <dependency org="javax.mail" name="mail" rev="1.2"/>
        <dependency org="jakarta" name="commons-collections" rev="3.2"/>
        <dependency org="jakarta" name="commons-httpclient" rev="3.0.1"/>
        <dependency org="jakarta" name="commons-pool" rev="1.3"/>        

		<!-- container -->
		<dependency org="postgresql" name="postgresql-jdbc3"
			rev="8.3-603" conf="container->master" />
		<dependency org="ehcache" name="ehcache" rev="1.2.4"
			conf="container->master" />

		<!-- test -->
		<dependency org="postgresql" name="postgresql-jdbc3"
			rev="8.3-603" conf="test->master" />
		<dependency org="org.antlr" name="antlr" rev="2.7.6"
			conf="test->master" />
		<dependency org="apache" name="log4j" rev="1.2.13"
			conf="test->master" />
		<dependency org="junit" name="junit" rev="4.1"
			conf="test->master" />
		
		<!-- other test targets -->
		<dependency org="checkstyle" name="checkstyle-all" rev="4.4"
			conf="test-checkstyle->runtime" />
		<dependency org="cobertura" name="cobertura" rev="1.9"
			conf="test-cobertura->runtime" />
		<dependency org="junit" name="junit" rev="4.1"
			conf="test-junit->runtime" />
		<dependency org="openqa" name="selenium-server" rev="1.0-beta-1"
			conf="test-selenium->runtime" />
		<dependency org="pmd" name="pmd" rev="4.2.2"
			conf="test-pmd->compile,runtime" />
		<dependency org="redhillconsulting" name="simian" rev="2.2.24"
			conf="test-simian->runtime" />

	</dependencies>
</ivy-module>
