<?xml version="1.0" encoding="UTF-8"?>
<project>
	<parent>
		<artifactId>caxchange</artifactId>
		<groupId>gov.nih.nci.caxchange</groupId>
		<version>2.0</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<artifactId>caXchange-beans-su</artifactId>
	<packaging>jbi-service-unit</packaging>
	<name>A servicemix-bean Service Unit</name>
	<url>http://www.myorganization.org</url>
	<build>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>**/*</include>
				</includes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.servicemix.tooling</groupId>
				<artifactId>jbi-maven-plugin</artifactId>
				<!--<version>${servicemix-version}</version>-->
				<version>3.2.3</version>
				<extensions>true</extensions>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<systemProperties>
						<property>
							<name>log4j.configuration</name>
							<value>
								file:target/test-classes/log4j.properties
							</value>
						</property>
					</systemProperties>
					<excludes>
						<exclude>
							**/StoreOriginalMessageDatabaseDownTest.*
						</exclude>
						<exclude>
							**/StoreOriginalMessageServiceBeanTest.*
						</exclude>
						<exclude>**/HandleErrorResponseBeanTest.*</exclude> 
						<!-- <exclude>**/GenerateResponseBeanTest.*</exclude> --> 
					    <exclude>**/CaxchangeAggregatorListenerTest.*</exclude>
						<exclude>
							**/InvokeDelegationServiceBeanTest.*
						</exclude> 
						 <exclude>**/InvokeDelegationServiceBeanInvalidCredentialsTest.*</exclude>
						  <exclude>**/CaXchangeDataUtilTest.*</exclude> 
						<exclude>
							**/CaxchangeConsumerInvocationTest.*
						</exclude>
						<exclude>**/PayloadValidationTest.*</exclude>
					</excludes>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>gov.nih.nci.caxchange</groupId>
			<artifactId>caXchange-security-jar</artifactId>
			<version>${project.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>gov.nih.nci.caxchange</groupId>
			<artifactId>caXchange-db-jar</artifactId>
			<version>${project.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>gov.nih.nci.caxchange</groupId>
			<artifactId>caXchange-common-jar</artifactId>
			<version>${project.version}</version>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>gov.nih.nci.caxchange</groupId>
			<artifactId>caXchange-outbound-core</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>gov.nih.nci.caxchange</groupId>
			<artifactId>caXchange-verify-routing-jar</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>org.apache.servicemix</groupId>
			<artifactId>servicemix-bean</artifactId>
			<!--<version>${servicemix-version}</version>-->
			<version>2008.01</version>
		</dependency>
		<dependency>
			<groupId>org.apache.servicemix</groupId>
			<artifactId>servicemix-core</artifactId>
			<version>${servicemix-version}</version>
			<scope>provided</scope>
		</dependency>
		<!-- Proxy caching dependencies -->
		<dependency>
		    <groupId>org.apache.jcs</groupId>
		    <artifactId>jcs-jar</artifactId>
		    <version>1.3</version>
		</dependency>
		<!-- End Proxy caching dependencies -->
		<dependency>
			<groupId>cagrid.cds</groupId>
			<artifactId>cds-client</artifactId>
			<version>1.2</version>
		</dependency>
		<dependency>
			<groupId>cagrid.cds</groupId>
			<artifactId>cds-common</artifactId>
			<version>1.2</version>
		</dependency>
		<dependency>
			<groupId>cagrid.cds</groupId>
			<artifactId>cds-stubs</artifactId>
			<version>1.2</version>
		</dependency>
		<dependency>
			<groupId>cagrid</groupId>
			<artifactId>gme</artifactId>
			<version>1.2</version>
		</dependency>
		<dependency>
			<groupId>cagrid</groupId>
			<artifactId>gme-stubs</artifactId>
			<version>1.2</version>
		</dependency>
    <!-- COPPA dependencies -->
        <dependency>
              <groupId>gov.nih.nci.coppa.po.grid</groupId> 
              <artifactId>CoppaPO-client</artifactId> 
              <version>1.0</version> 
        </dependency> 
        <dependency>
              <groupId>gov.nih.nci.coppa.po.grid</groupId> 
              <artifactId>CoppaPO-common</artifactId> 
