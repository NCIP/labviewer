  <packaging>jbi-service-unit</packaging>
  <name>A servicemix-bean Service Unit</name>
  <url>http://www.myorganization.org</url>
  <build>
    <resources>
      <resource>
        <directory>src/main/resources</directory>
        <includes>
          <include>**/*</include>
        </includes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.servicemix.tooling</groupId>
        <artifactId>jbi-maven-plugin</artifactId>
        <version>${servicemix-version}</version>
        <extensions>true</extensions>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <systemProperties>
            <property>
              <name>log4j.configuration</name>
              <value>file:target/test-classes/log4j.properties</value>
            </property>
          </systemProperties>          
          <excludes>
            <exclude>**/StoreOriginalMessageServiceBeanDatabaseDownTest.*</exclude>
            <exclude>**/StoreOriginalMessageServiceBeanTest.*</exclude>
            <exclude>**/HandleErrorResponseBeanTest.*</exclude>
            <exclude>**/GenerateResponseBeanTest.*</exclude>
            <exclude>**/CaxchangeAggregatorListenerTest.*</exclude> 
             <exclude>**/InvokeDelegationServiceBeanTest.*</exclude>
             <exclude>**/XPathUtilTest.*</exclude>
             <exclude>**/CaxchangeConsumerInvocationTest.*</exclude>  
            <exclude>**/PayloadValidationTest.*</exclude>
          </excludes>
        </configuration>
      </plugin>      
    </plugins>
  </build>
